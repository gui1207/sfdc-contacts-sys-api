	<global-property doc:name="Global Property" doc:id="312f4bd0-c20f-4182-aced-a003674bbeeb" name="mule.env" value="dev" />
	<configuration-properties doc:name="Configuration properties" doc:id="f2571785-28f4-413b-9033-ae8b4b09ba14" file="properties/${mule.env}-properties.yaml" />
	
</mule>
